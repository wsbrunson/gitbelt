{
  "name": "gitbelt",
  "version": "0.1.0",
  "description": "Utility belt for user-friendly management of git",
  "main": "index.js",
  "repository": "https://github.com/wsbrunson/gitbelt",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "lint": "eslint --fix .",
    "test": "yarn lint && jest"
  },
  "bin": {
    "gitbelt": "index.js"
  },
  "keywords": ["git", "help", "utility", "branch", "belt", "delete"],
  "author": "Shane Brunson",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "commitlint": "^7.5.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "prettier": "^1.16.3"
  },
  "dependencies": {
    "inquirer": "^6.2.2",
    "nodegit": "^0.24.0",
    "ramda": "^0.26.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branch": "master"
  },
  "commitlint": { "extends": ["@commitlint/config-conventional"] }
}
